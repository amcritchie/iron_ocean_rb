<%= render 'layouts/headers/admin_header' %>
<div class="container" style="margin-top: 50px;">
  <div class="col-md-8">
    <% @blogs.each do |blog| %>
      <h2><%= blog.title %></h2>
      
      <%= render "blogs/library/#{blog.slug}" %>

    <% end %>
  </div>
  <div class="col-md-4 list-group">
    Check out our blogs.

  </div>
</div>
