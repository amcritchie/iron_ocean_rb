<%= form_for :user do |f| %>


    <div class="field">
      <%= f.label :sender_id %><br>
      <%= f.number_field :sender_id %>
    </div>
    <div class="field">
      <%= f.label :receiver_id %><br>
      <%= f.number_field :receiver_id %>
    </div>
    <div class="field">
      <%= f.label :title %><br>
      <%= f.text_field :title %>
    </div>
    <div class="field">
      <%= f.label :body %><br>
      <%= f.text_area :body %>
    </div>
    <div class="actions">
      <%= f.submit %>
    </div>
<% end %>