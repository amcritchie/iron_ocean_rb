<div class="container">
  <div class="row">
    <div class="col-sm-2 col-lg-3"></div>
    <div class="col-sm-8 col-lg-6">
      <h1>Contact</h1>
    </div>
    <div class="col-sm-2 col-lg-3"></div>
  </div>
  <div class="col-sm-2 col-lg-3"></div>
  <div class="col-sm-8 col-lg-6">
    <form class="contactForm left" role="form" action="/" method="get" accept-charset="UTF-8">
      <div class="form-group">
        <label for="email">Email:</label>
        <input name="email" type="email" class="form-control" id="contact_email" autofocus>
      </div>
      <div class="form-group">
        <label for="message">Message:</label>
        <textarea name="message" class="form-control" id="contact_message"></textarea>
      </div>
      <button type="submit" class="btn btn-primary col-xs-6"><i class="fa fa-spin fa-spinner loading"></i><span class="resting">Submit</span><i class="fa fa-spinner fa-spin fa-1x waiting-view"></i></button>
    </form>
  </div>
  <div class="col-sm-2 col-lg-3"></div>
</div>
</div>
